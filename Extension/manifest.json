{
  "name": "Suffix Search",
  "version": "1.0",
  "description": "Customize your Google search with a suffix.",
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
